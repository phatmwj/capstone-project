<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="a"
            type="winwin.customer.app.ui.splashform.SplashFormActivity" />
        <variable
            name="vm"
            type="winwin.customer.app.ui.splashform.SplashFormViewModel" />
    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:theme="@style/Welcome"
        android:background="@color/welcome_color"
        tools:context=".ui.splashform.SplashFormActivity">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_100sdp"
            android:layout_marginHorizontal="@dimen/_50sdp"
            android:layout_marginVertical="0dp"
            android:src="@drawable/logo_name" />
    </RelativeLayout>
</layout>